{
  "name": "as4-generating-types-example",
  "version": "1.0.0",
  "description": "An example of generating types from a schema to use in your resolvers.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml",
    "compile": "npm run generate && tsc",
    "postinstall": "npm run compile",
    "build": "npm run compile",
    "start": "node ./dist/index.js",
    "dev": "npm run compile && dotenv -e .env.dev -- node ./dist/index.js",
    "watch": "dotenv -e .env.dev concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\"  \"tsc -w \" \"nodemon ./dist/index.js \"",
    "test": "dotenv -e .env.test -- jest",
    "test:update": "jest -u",
    "prisma-generate-dev": "dotenv -e .env.dev -- npx prisma generate",
    "prisma-generate-test": "dotenv -e .env.test -- npx prisma generate",
    "prisma-migrate-dev": "dotenv -e .env.dev -- npx prisma generate && dotenv -e .env.dev -- npx prisma db push",
    "prisma-migrate-test": "dotenv -e .env.test -- npx prisma generate && dotenv -e .env.test -- npx prisma db push",
    "prisma-migrate-prod": "npx prisma generate && npx prisma db push",
    "add-existing-public-cloud-projects": "dotenv -e .env -- node ./dist/scripts/newPublicCloudProjects/index.js",
    "update-users": "dotenv -e .env -- node ./dist/scripts/updateUsers/index.js",
    "idir-upn-users-update": "dotenv -e .env -- node ./dist/scripts/idirUpnUserUpdate/index.js",
    "update-creation-dates": "dotenv -e .env -- node ./dist/scripts/updateCreationDates/index.js",
    "update-common-components": "dotenv -e .env -- node ./dist/scripts/updateCommonComponents/index.js",
    "update-requests": "dotenv -e .env -- node ./dist/scripts/updateRequests/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.0.0",
    "@azure/msal-node": "^1.17.2",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@octokit/auth": "^3.0.3",
    "@octokit/core": "^4.2.0",
    "@prisma/client": "^5.0.0",
    "axios-oauth-client": "^1.5.0",
    "axios-token-interceptor": "^0.2.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "crypto": "^1.0.1",
    "dataloader": "^2.1.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.3.14",
    "jsonwebtoken": "^9.0.0",
    "keycloak-connect": "^20.0.2",
    "keycloak-connect-graphql": "^0.7.0",
    "lodash.isequal": "^4.5.0",
    "mongodb": "^4.13.0",
    "nats": "^2.6.1",
    "node-cron": "^3.0.2",
    "octokit": "^2.0.14",
    "swig": "^1.4.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-mongodb": "^2.4.6",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@graphql-tools/mock": "^8.7.14",
    "@graphql-tools/schema": "^9.0.12",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "concurrently": "7.4.0",
    "graphql-codegen-typescript-validation-schema": "^0.7.1",
    "jest": "^29.3.1",
    "jest-preview": "^0.3.1",
    "nodemon": "2.0.19",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
