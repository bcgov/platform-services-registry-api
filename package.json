{
  "name": "as4-generating-types-example",
  "version": "1.0.0",
  "description": "An example of generating types from a schema to use in your resolvers.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml",
    "compile": "npm run generate && tsc",
    "postinstall": "npm run compile",
    "start": "npm run migrate-prod && npm run compile && node ./dist/index.js",
    "dev": "npm run compile && dotenv -e .env.dev -- node ./dist/index.js", 
    "watch": "dotenv -e .env.dev concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\"  \"tsc -w \" \"nodemon ./dist/index.js \"",
    "test": "jest",
    "generate-dev": "dotenv -e .env.dev -- npx prisma generate",
    "migrate-dev": "dotenv -e .env.dev -- npx prisma generate && dotenv -e .env.dev -- npx prisma db push",
    "migrate-test": "dotenv -e .env.test -- npx prisma generate && dotenv -e .env.test -- npx prisma db push",
    "migrate-prod": "npx prisma generate && npx prisma db push",
    "dev:test": "rm -rf ./dist && npm run compile && jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.0.0",
    "@prisma/client": "^4.8.1",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dataloader": "^2.1.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.3.14",
    "keycloak-connect": "^20.0.2",
    "keycloak-connect-graphql": "^0.7.0",
    "mongodb": "^4.13.0",
    "nats": "^2.10.2",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-mongodb": "^2.4.6",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@graphql-tools/mock": "^8.7.14",
    "@graphql-tools/schema": "^9.0.12",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "concurrently": "7.4.0",
    "jest": "^29.3.1",
    "nodemon": "2.0.19",
    "prisma": "^4.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
